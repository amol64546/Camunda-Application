<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
    targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="apiOrchestrationProcess" name="API Orchestration Process" isExecutable="true">
    <bpmn:startEvent id="startEvent" />
    <bpmn:sequenceFlow id="flow1" sourceRef="startEvent" targetRef="apiCallTask" />
    <bpmn:serviceTask id="apiCallTask" name="Call API Task" camunda:class="com.example.camunda.delegates.ApiCallDelegate" />
    <bpmn:sequenceFlow id="flow2" sourceRef="apiCallTask" targetRef="endEvent" />
    <bpmn:endEvent id="endEvent" />
  </bpmn:process>
</bpmn:definitions>
